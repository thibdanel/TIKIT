<div class="container">
  <div class="row edit-us-show" data-controller= "flatpicker">
    <%= simple_form_for([@user_service, @task]) do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :end_date, as: :string, required: false, input_html: {class: "datepicker"} %>

    <%= f.button :submit, "Edit", id: "link-us-edit" %>
    <% end %>
  </div>
</div>
